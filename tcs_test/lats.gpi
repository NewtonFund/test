# Reported versus 1st observed magnitudes
#

DEBUG = 0 
PAUSE = 1

#
set autoscale
unset log
unset label
set key top right font ",10"
set xtic auto
set ytic auto
set title  "NOVAS/SOFA - ZD/AZ Difference for S. Latitudes (Long. = 0)"  
set xlabel "Latitude [degrees]" 
set ylabel "Delta [arcsec]" 

#set yrange [-5:+5] 
#set xrange [9]

if (DEBUG) {
  set terminal x11 enhanced font "mono,14"   
} else {
  set terminal png truecolor nocrop enhanced font "mono,10"   
  set output "lats.png"
}

plot 'lats.dat' u 1:(($2-$5)*3600) title "ZD" w lines lc rgb "red",\
     ''         u 1:(($3-$6)*3600) title "AZ" w lines lc rgb "blue"

#pause -1 
